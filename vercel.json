{
  "version": 2,
  "crons": [
    {
      "path": "/api/cron/",
      "schedule": "30 13 * * *"
    }
  ],
  "builds": [
    {
      "src": "aimedic/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "static_root"
      }
    }
  ],
  "routes": [
    {
      "src": "/static_root/(.*)",
      "dest": "/static_root/$1"
    },
    {
      "src": "/(.*)",
      "dest": "aimedic/wsgi.py"
    }
  ]
}
